<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wutrial">

    <uses-permission android:name="android.permission.SEND_SMS"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/actionbar_icon"
        android:logo="@drawable/wu_prim"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_icon"
        android:supportsRtl="true"
        android:noHistory="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PaymentPinActivity"/>
        <activity android:name=".PasscodeActivity"/>
        <activity android:name=".MainActivity"/>
        <activity android:name=".AddMoneyActivity"/>
        <activity android:name=".SendMoneyActivity"/>
        <activity android:name=".SetPasscodeActivity"/>
        <activity android:name=".SetPaymentPinActivity"/>
        <activity android:name=".RegisterActivity2"/>
        <activity android:name=".RegisterActivity1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>